   "source": [
    "# Write a function that takes three variables x, y, and z and returns the result of the expression x^2 + y^2 - z^2 + 2xy - 2yz + 2zx."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "26\n"
     ]
    }
   ],
   "source": [
    "def res(x,y,z):\n",
    "    final_res = x**2 + y**2 - z**2 + 2*x*y - 2*y*z + 2*z*x\n",
    "    print(final_res)\n",
    "\n",
    "res(3,2,1)"
   ]
  }
